
pipeline{
     agent {
        docker {image 'python:latest'}
    }
    stages{
        stage('run'){
            steps{
                sh 'sudo pip install pyinstaller'
                sh 'sudo pyinstaller --onefile pie.py'
            }
        }
    }
    post{
         success{
            archiveArtifacts artifacts: 'dist/pie.exe'
        }
    }
   
}
